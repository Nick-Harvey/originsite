      / Header Section
      %header.index_static
        .nav_logo.animated.fadeIn
          .container
            .row
              %nav.col-md-12
                %ul#menu.sf-menu
                  %li 
                    %a{:href => "index.html#try"} TRY OPENSHIFT
                  %li
                    %a{:href => "index.html#v3"} WHAT'S COMING NEXT
                  %li 
                    %a{:href => "index.html#contribute"} CONTRIBUTE
                  %li
                    %a{:href => "http://openshift.github.io/documentation/"} DOCUMENTATION
